<ul class="fixtures unstyled">
{{#each fixture in controller}}
  {{#view tagName="li" classNameBindings="fixture.homeOrAway disableHomeTeam disableAwayTeam selectionClass"}}
    <a class="home" {{action selectWinner fixture fixture.homeTeam}}>
      {{fixture.homeTeam.name}}
    </a>
    <div class="vs">
      vs
    </div>
    <a class="away" {{action selectWinner fixture fixture.awayTeam}}>
      {{fixture.awayTeam.name}}
    </a>
  {{/view}}
{{/each}}
</ul>